#include <stdio.h>

int main() {
    int s1, s2, s3, s4, s5;
    
    printf("Enter marks in 5 subjects: ");
    scanf("%d %d %d %d %d", &s1, &s2, &s3, &s4, &s5);
    
    printf("Marks in Subject 1: %d\n", s1);
    printf("Marks in Subject 2: %d\n", s2);
    printf("Marks in Subject 3: %d\n", s3);
    printf("Marks in Subject 4: %d\n", s4);
    printf("Marks in Subject 5: %d\n", s5);
    
    return 0;
}