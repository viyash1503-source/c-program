#include <stdio.h>

int main() {
    int num, lastDigit, nextDigit, flag = 1;

    printf("Enter a number: ");
    scanf("%d", &num);
    lastDigit = num % 10;
    num /= 10;

    while (num > 0) {
        nextDigit = num % 10;
        if (nextDigit > lastDigit) {  // Not ascending if left digit > right digit
            flag = 0;
            break;
        }
        lastDigit = nextDigit;
        num /= 10;
    }

    if (flag)
        printf("Digits are in ascending order.\n");
    else
        printf("Digits are not in ascending order.\n");

    return 0;
}